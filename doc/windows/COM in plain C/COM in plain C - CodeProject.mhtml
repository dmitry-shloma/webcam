typedef HRESULT STDMETHODCALLTYPE QueryInterfacePtr(IExample *, REFIID, <sp=
an class=3D"code-keyword" wfd-id=3D"770">void</span> **);
typedef ULONG STDMETHODCALLTYPE AddRefPtr(IExample *);
typedef ULONG STDMETHODCALLTYPE ReleasePtr(IExample *);

typedef <span class=3D"code-keyword" wfd-id=3D"769">struct</span> {
