: 4; font-size: 12px !important; line-height: 15px !important; z-index: 0; =
opacity: 0; overflow: hidden;">// Initialise everything.
SDL_Init(SDL_INIT_VIDEO);
IMG_Init(IMG_INIT_JPG);

// Get the screen's surface.
SDL_Surface* screen =3D SDL_SetVideoMode(
    format.fmt.pix.width,
    format.fmt.pix.height,
    32, SDL_HWSURFACE
);

SDL_RWops* buffer_stream;
SDL_Surface* frame;
SDL_Rect position =3D {.x =3D 0, .y =3D 0};

// Create a stream based on our buffer.
buffer_stream =3D SDL_RWFromMem(buffer_start, bufferinfo.length);

// Create a surface using the data coming out of the above stream.
frame =3D IMG_Load_RW(buffer_stream, 0);

// Blit the surface and flip the screen.
SDL_BlitSurface(frame, NULL, screen, &amp;position);
SDL_Flip(screen);

// Free everything, and unload SDL &amp; Co.
SDL_FreeSurface(frame);
SDL_RWclose(buffer_stream);
IMG_Quit();
SDL_Quit();</textarea></div>
