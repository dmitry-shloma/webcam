opacity: 0; overflow: hidden;">struct v4l2_requestbuffers bufrequest;
bufrequest.type =3D V4L2_BUF_TYPE_VIDEO_CAPTURE;
bufrequest.memory =3D V4L2_MEMORY_MMAP;
bufrequest.count =3D 1;

if(ioctl(fd, VIDIOC_REQBUFS, &amp;bufrequest) &lt; 0){
    perror("VIDIOC_REQBUFS");
    exit(1);
}</textarea></div>
